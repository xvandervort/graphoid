# Test starts_with and ends_with methods
text = "Hello World"

# Test starts_with
print("Testing starts_with:")
print("text = \"Hello World\"")
print("starts_with(\"Hello\"): " + text.starts_with("Hello").to_string())
print("starts_with(\"World\"): " + text.starts_with("World").to_string())
print("starts_with(\"Hi\"): " + text.starts_with("Hi").to_string())

# Test ends_with
print("\nTesting ends_with:")
print("ends_with(\"World\"): " + text.ends_with("World").to_string())
print("ends_with(\"Hello\"): " + text.ends_with("Hello").to_string())
print("ends_with(\"ld\"): " + text.ends_with("ld").to_string())

# Test with empty strings
empty = ""
print("\nTesting with empty string:")
print("\"Hello\".starts_with(\"\"): " + text.starts_with("").to_string())
print("\"\".starts_with(\"Hello\"): " + empty.starts_with("Hello").to_string())

# Test case sensitivity
print("\nTesting case sensitivity:")
print("\"Hello\".starts_with(\"hello\"): " + text.starts_with("hello").to_string())
print("\"World\".ends_with(\"WORLD\"): " + text.ends_with("WORLD").to_string())