# Test lambda expressions

# Simple lambda
double = x => x * 2
result = double(5)
print "double(5) = " + result.to_string()

# Lambda with multiple parameters  
add = (x, y) => x + y
sum_result = add(3, 7)
print "add(3, 7) = " + sum_result.to_string()

# Lambda assigned and called
square = x => x * x
direct_result = square(4)
print "Square of 4 = " + direct_result.to_string()