#!/usr/bin/env glang

# Test if built-in operations accept custom lambdas

print("Testing custom lambdas with built-in operations")

numbers = [1, 2, 3, 4, 5]

# Try to use custom lambda with map
print("Testing list.map with custom lambda:")
custom_transform = x => x * 3
try_map = numbers.map(custom_transform)
print("map with lambda: " + try_map.to_string())

# Try to use custom lambda with filter
print("Testing list.filter with custom lambda:")
is_even = x => (x % 2) == 0
try_filter = numbers.filter(is_even)
print("filter with lambda: " + try_filter.to_string())

print("Testing completed")